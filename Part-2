  @Test
  public void amplience2() {
    driver.get("https://github.com/6wl");
    driver.manage().window().setSize(new Dimension(1616, 876));
    assertThat(driver.findElement(By.cssSelector(".p-name")).getText(), is("Gregory Loscombe"));
    assertThat(driver.findElement(By.cssSelector(".mb-3 > .Link--secondary:nth-child(1)")).getText(), is("15 followers"));
    assertThat(driver.findElement(By.cssSelector(".Link--secondary:nth-child(2)")).getText(), is("29 following"));
    assertThat(driver.findElement(By.cssSelector(".p-label")).getText(), is("Manchester"));
    assertThat(driver.findElement(By.cssSelector(".width-full > .UnderlineNav-body > .UnderlineNav-item:nth-child(2)")).getText(), is("Repositories\\\\n7"));
  }
